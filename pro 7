                               find all angles of a triangle if two angles are given

#include <stdio.h>
int main()
{
int a, b, c;

    printf("Enter first angles of triangle: ");
    scanf("%d", &a);

    printf("Enter second  angles of triangle: ");
    scanf("%d", &a);

    c = 180 - (a + b);
    printf("angle of the triangle = %d", c);
    return 0;
}
